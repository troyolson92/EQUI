            @Html.DisplayFor(modelItem => item.wiki1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.searchtags)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Comments)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.isDead)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.createDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.l_user_id)
        </td>
