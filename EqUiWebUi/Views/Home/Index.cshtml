@{
    ViewBag.Title = "VCG GA Homepage";
}
<div class="jumbotron">
    <h1>WELCOME IN VCG GA</h1>
</div>

<p></p>

<div class="card">
    <div class="card-header bg-dark">
        <h5 class="card-title text-white">
            Supervisie tools algemeen GA
            <span class="clickable float-right"><i class="fas fa-chevron-circle-up fa-1"></i></span>
        </h5>
    </div>
    <div class="card-body">
        <p>
            <a href="~/Gadata/Table/PloegRapportWebgrid" class="btn btn-primary btn-sm">PloegRapport &raquo;</a>
            Bevat storingen groter dan 20 minuten of die meer dan 3 keer zijn voorgevallen
        </p>
        <p>
            <a href="~/Gadata/Table/SupervisieWebgrid" class="btn btn-primary btn-sm">Supervisie Robots &raquo;</a>
            Bevat alles nu in storing en storingen die langer dan 1 minut hebben geduurd.
        </p>

        @{
            string urlIntranetSupervis = @Uri.EscapeDataString("http://webapps.gen.volvocars.net/applications/eng/equipment/supervisiep1/intranet");
            <p>
                <a href="~/Home/Rendersite?url=@urlIntranetSupervis" class="btn btn-primary btn-sm">Supervisie Intranet P1 &raquo;</a>
                Supervisie (intranet)
            </p>
        }

        <p>
            <a href="~/Alert/Alert/Index" class="btn btn-primary btn-sm">Alerts &raquo;</a>
            Waarschuwingen gebaseerd op triggers
        </p>
    </div>
</div>

<p></p>

<div class="card">
    <div class="card-header bg-dark">
        <h5 class="card-title text-white">
            Greenfield Only
            <span class="clickable float-right"><i class="fas fa-chevron-circle-up fa-1"></i></span>
        </h5>
    </div>
    <div class="card-body">
        <p>
            <a href="~/Tiplife/NgacElectrode/index" class="btn btn-primary btn-sm">Electrode &raquo;</a>
            Alles in verband met de Electrodes van de nieuwe robots
        </p>

        <p>
            <a href="http://gensvw2107.gen.volvocars.net/Greenfield/#/ctgreenfield" class="btn btn-primary btn-sm">cmaCTapp &raquo;</a>
            Tool van IE om Cycle times te bekijken.
        </p>

        <p>
            <a href="~/PlcSupervisie/Screenshot/Carousel" class="btn btn-primary btn-sm">UB12 supervisie &raquo;</a>
            Plc Supervisie voor UB12 (mobile friendly)
        </p>

        @{
            string urlGreenfieldsuperVis = @Uri.EscapeDataString("http://webapps.gen.volvocars.net/applications/eng/equipment/SuperVisieCma/");
            <p>
                <a href="~/Home/Rendersite?url=@urlGreenfieldsuperVis" class="btn btn-primary btn-sm">Tia supervisie index &raquo;</a>
                Elk beschikbaar scherm van CMA supervisie.
            </p>
        }
    </div>
</div>

<p></p>

