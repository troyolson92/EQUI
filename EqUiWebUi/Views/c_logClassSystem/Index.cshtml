@model IEnumerable<EqUiWebUi.Models.c_logClassSystem>
<h3>c_logClassSystem</h3>

<p>
    @Html.ActionLink("Create New", "EDIT",new {id = -1 })
</p>
@Html.Grid(Model).Named("c_logClassSystemGrid").Build(columns =>
{
    columns.Add(c => c.id).Titled("id").InitialSort(GridSortOrder.Asc);
    columns.Add(c => c.Name).Titled("Name");
    columns.Add(c => c)
            .Titled("")
            .Encoded(false)
        .RenderedAs(c => @Html.ActionLink("Edit", "Edit", new { id = c.id }) + " | " +
                         @Html.ActionLink("Delete", "Delete", new { id = c.id })
            ).Filterable(false);
}).Sortable().Pageable(pager =>
{
    pager.RowsPerPage = 10;
}).Empty("Did not find anything").Css("table-hover")


