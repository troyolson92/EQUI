@model IQueryable<EqUiWebUi.Models.l_dummyLogClassResult>

@Html.Grid(Model).Named("dummyLogClassResultGrid").Build(columns =>
{
    columns.Add(c => c.id).Titled("id").InitialSort(GridSortOrder.Asc);
    columns.Add(c => c.code).Titled("Code");
    columns.Add(c => c.text).Titled("Text");
    columns.Add(c => c.c_Classification.Classification).Titled("Class");
    columns.Add(c => c.c_Subgroup.Subgroup).Titled("Subgroup");
    columns.Add(c => c.c_logcClassRules_id).Titled("RuleID");
}).Sortable().Pageable(pager =>
{
    pager.RowsPerPage = 10;
    pager.PageSizes = new Dictionary<Int32, String> { { 0, "All" }, { 10, "10" }, { 20, "20" }, { 40, "40" }, { 80, "80" }, { 100, "100" } };
    pager.ShowPageSizes = true;
}).Empty("Did not find anything").Css("table-hover")

