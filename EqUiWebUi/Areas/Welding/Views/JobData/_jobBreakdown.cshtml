@model IQueryable<EqUiWebUi.Areas.Welding.Models.rt_job_breakdown1>

@Html.Grid(Model).Named("breakdownGrid").Build(columns =>
{
    columns.Add(c => c.id).Titled("id");
    columns.Add(c => c.index).Titled("index");
    columns.Add(c => c.ts_breakdownStart).Titled("ts_Start").InitialSort(GridSortOrder.Desc);
    columns.Add(c => c.ts_breakdownEnd).Titled("ts_End");
    //





}).Filterable().Sortable().Pageable(pager =>
{
    pager.RowsPerPage = 20;
    pager.PageSizes = new Dictionary<Int32, String> { { 0, "All" }, { 20, "20" }, { 40, "40" }, { 80, "80" }, { 100, "100" } };
    pager.ShowPageSizes = true;
}).Empty("Did not find anything").Css("table-hover")
