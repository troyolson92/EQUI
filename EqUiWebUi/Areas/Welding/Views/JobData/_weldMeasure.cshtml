@model IQueryable<EqUiWebUi.Areas.Welding.Models.rt_weldmeasureprotddw>

@Html.Grid(Model).Named("weldMeasureGrid").Build(columns =>
{
    columns.Add(c => c.id).Titled("id");
    columns.Add(c => c.dateTime).Titled("datetime").InitialSort(GridSortOrder.Desc);
    columns.Add(c => c.spotId).Titled("spotId");
    columns.Add(c => c.progNo).Titled("progNo");
    columns.Add(c => c.electrodeNo).Titled("electrode");
    columns.Add(c => c.tipDressCounter).Titled("dressCounter");

    columns.Add(c => c.energyActualValue).Titled("energy");
    columns.Add(c => c.resistanceActualValue).Titled("resistance");
    columns.Add(c => c.weldTimeActualValue).Titled("WeldTime");
    columns.Add(c => c.stabilisationFactorActValue).Titled("Psf");
    columns.Add(c => c.uipActualValue).Titled("uip");
    columns.Add(c => c.uirQStoppActCntValue).Titled("QstopValue");
    columns.Add(c => c.uirExpulsionTime).Titled("SpatterTime");





}).Filterable().Sortable().Pageable(pager =>
{
    pager.RowsPerPage = 20;
    pager.PageSizes = new Dictionary<Int32, String> { { 0, "All" }, { 20, "20" }, { 40, "40" }, { 80, "80" }, { 100, "100" } };
    pager.ShowPageSizes = true;
}).Empty("Did not find anything").Css("table-hover").RowCss(c => c.uirMeasuringActive == 0 || c.regulation2 == 2 && c.uirWeldTimeProlongationActive == 1 ? "ani_PulseRed" : "") 