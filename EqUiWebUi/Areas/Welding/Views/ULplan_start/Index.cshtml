@model IEnumerable<EqUiWebUi.Areas.Welding.Models.ULplan_start>

@{
    ViewBag.Title = "Index";
 
}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.planid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.lastplannumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Startinspectiontime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.planlenght)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.inspectorid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StationId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Worklocation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.bodynr)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.plancounter)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.planid)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.lastplannumber)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Startinspectiontime)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.planlenght)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.inspectorid)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.StationId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Worklocation)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.bodynr)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.plancounter)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
                    @Html.ActionLink("Details", "Details", new { id = item.id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.id })
                </td>
            </tr>
        }

    </table>
</body>


</html>
