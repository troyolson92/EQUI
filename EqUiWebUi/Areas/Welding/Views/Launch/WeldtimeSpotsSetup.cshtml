
@model IQueryable<EqUiWebUi.Areas.Welding.Models.WeldtimeSpotsSetup>
@{
    ViewBag.Title = "WeldtimeSpotsSetup";
}


<h2>DoubleSpotCheck</h2>

@Html.Grid(Model).Named("WeldtimeSpotsSetup").Build(columns =>
{
    columns.Add(c => c.NPT).Titled("NPT").InitialSort(GridSortOrder.Desc);
    columns.Add(c => c.Timer).Titled("TimerName");
    columns.Add(c => c.spotname).Titled("Spotname");
    columns.Add(c => c.Expr1).Titled("TotalWeldtime");



}).Css("css-classes").AppendCss("table-hover").Filterable().Pageable(pager =>
{
    pager.RowsPerPage = 20;

}
    ).Sortable().Css("table-hover");

@section scripts{
    <script>
        //full with screen
        $("#allcontent").removeClass("body-content");
        $("#allcontent").removeClass("container");
    </script>
}


