
@model IQueryable<EqUiWebUi.Areas.Welding.Models.CheckDubbelPrograms>
@{
    ViewBag.Title = "CheckDubbelPrograms";
}


<h2>Check for double programs used</h2>

@Html.Grid(Model).Named("CheckDubbelPrograms").Build(columns =>

{
    columns.Add(c => c.id).Titled("id");
    columns.Add(c => c.Name).Titled("TimerName").InitialSort(GridSortOrder.Asc);
    columns.Add(c => c.SpotName).Titled("spotName");
    columns.Add(c => c.weldProgNo).Titled("ProgramNbr");
    columns.Add(c => "double program is used in weldtimer!!").Titled("text");



}).Css("css-classes").AppendCss("table-hover").Filterable().Pageable(pager =>
{
    pager.RowsPerPage = 20;

}
    ).Sortable().Css("table-hover").RowCss(model => model.Name != null ? "ani_PulseBlueRepeat" : "");

@section scripts{
    <script>
        //full with screen
        $("#allcontent").removeClass("body-content");
        $("#allcontent").removeClass("container");
    </script>
}

