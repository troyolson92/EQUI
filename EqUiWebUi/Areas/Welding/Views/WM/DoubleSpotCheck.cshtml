
@model IQueryable<EqUiWebUi.Areas.Welding.Models.DoubleSpotCheck>
@{
    ViewBag.Title = "DoubleSpotCheck";
}


<h2>DoubleSpotCheck</h2>

@Html.Grid(Model).Named("DoubleSpotCheck").Build(columns =>
{
    columns.Add(c => c.TimerName).Titled("TimerName");
    columns.Add(c => c.WeldProgram).Titled("WeldProgram");
    columns.Add(c => c.SpotName).Titled("Spotname");
 


}).Css("css-classes").AppendCss("table-hover").Filterable().Pageable(pager =>
{
    pager.RowsPerPage = 20;
    pager.PageSizes = new Dictionary<Int32, String>
    { { 0, "All" } };
    //{ 20, "20" }, { 40, "40" }, { 80, "80" }, { 100, "100" } };
    pager.ShowPageSizes = true;
    }
    ).Sortable();

    @section scripts{
        <script>
            //full with screen
            $("#allcontent").removeClass("body-content");
            $("#allcontent").removeClass("container");
        </script>
    }
