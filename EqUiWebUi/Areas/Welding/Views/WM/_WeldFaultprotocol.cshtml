@model IEnumerable<EqUiWebUi.Areas.Welding.Models.WeldFaultProtocol>

@Html.Grid(Model).Named("_WeldFaultprotocol").Build(columns =>
{
    columns.Add(c => c.id).Titled("id");
    columns.Add(c => c.NPT).Titled("NPT");
    columns.Add(c => c.Name).Titled("timerName");
    columns.Add(c => c.errorCode1_txt).Titled("Fault");
    columns.Add(c => c.FaultStart).Titled("Fault").InitialSort(GridSortOrder.Desc);
    columns.Add(c => c.breakdownTime).Titled("BreakdownTime");
    columns.Add(c => c.CountFaultsInJob).Titled("CountFaultsInJob");



    columns.Add(c => c.progNo).Titled("Timerprogram");
    // columns.Add(c => c.spotId).Titled("Spotid");
    // columns.Add(c => "<p id=" + c.id + " class='editTestCollum2'>" + c.Timer).Encoded(false).Titled("Timertest");

    columns.Add(c => "<p id=" + c.id + " class='comment'>" + c.WMComment).Encoded(false).Titled("WMComment");
    columns.Add(c => c)
                 .Titled("")
                 .Encoded(false)
                 .RenderedAs(c => MyFilters.IconActionLink(Url.Action("Edit", new { id = c.id }), "fas fa-pencil-alt", "", "Edit this"));


}).Filterable().Pageable(pager =>
{
    pager.RowsPerPage = 10;

}).Sortable().Css("table-hover")
