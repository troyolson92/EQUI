    columns.Add(c => c.lastWeldShift).Titled("lastWeldShift");
    columns.Add(c => c.nbrWelds).Titled("nbrWelds");
    columns.Add(c => c.SpatterCount).Titled("spatterCount");
    columns.Add(c => c.ReweldCount).Titled("reweldcount");
    columns.Add(c => c.AVGpsf).Titled("AVGpsf");
    columns.Add(c => c.Avg_resistance).Titled("Avg_resistance");


}).Filterable().Pageable(pager =>
{
    pager.RowsPerPage = 20;

}
        ).Sortable().Css("table-hover");



