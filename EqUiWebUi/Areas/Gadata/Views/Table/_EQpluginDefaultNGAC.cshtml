@model IEnumerable<EqUiWebUi.Areas.Gadata.Models.EQpluginDefaultNGAC_Result>

@Html.Grid(Model).Named("EQpluginDefaultNGAC").Build(columns =>
{
    columns.Add(c => c.Location).Titled("Location").Filterable(true);
    columns.Add(c => c.logtext).Titled("Logtext").Css("xxd-none xxd-sm-block");
    columns.Add(c => c.Downtime_min_).Titled("DT");
    columns.Add(c => c.time).Titled("time");
    columns.Add(c => c.Classification).Titled("Class");
    columns.Add(c => c.Subgroup).Titled("Subgroup");
    columns.Add(c => c.Logtype).Titled("LogType").Filterable(true);
    columns.Add(c => c.LocationTree).Filterable(true).Css("d-none");
    columns.Add(c => c.Logcode).Css("d-none");
    columns.Add(c => c.refId).Css("d-none");
}).Sortable().Pageable(pager =>
{
    pager.RowsPerPage = 20;
}).Empty("No data found").Css("table-hover").RowCss(row => MyRowStyling.getRowStyleLogtype(row.Logtype))
