@model EqUiWebUi.Models.LogInfo

<div class="card">
    <div class="card-header bg-light">
        <h6 class="card-title">
            Details for logcode: <strong>@Model.errornum</strong> logtext: <strong>@Model.logtext.Trim()</strong>
            <span class="clickable float-right text-black-50"><i class="fas fa-chevron-circle-up fa-1"></i></span>
        </h6>
    </div>
    <div class="card-body">
        @Html.Raw(Model.logDetails)
    </div>
</div>