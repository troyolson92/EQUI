@model string
@{
    ViewBag.Title = "ViewAsUser";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="label">
    <div style="z-index: 2; position: absolute; top: 0px; left: 0px;">
        <h6>Viewing As @HttpContext.Current.Session["Impersonating"]</h6>
        @Html.ActionLink("End impresonating", "ResetCookie","User")
    </div>
</div>

<body style="margin:0px;padding:0px">
    <iframe src="@Model" frameborder="0" style="overflow:hidden;overflow-x:hidden;height:150%;width:100%;position:absolute;top:0px;left:0px;right:0px;bottom:0px" height="150%" width="150%"></iframe>
</body>
