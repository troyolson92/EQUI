@model IQueryable<EqUiWebUi.Areas.VWSC.Models.VWSC_rt_job_breakdown>

@Html.Grid(Model).Named("breakdownGrid").Build(columns =>
{
    columns.Add(c => c.id).Titled("id").InitialSort(GridSortOrder.Desc);
    columns.Add(c => c.index).Titled("idx");
    columns.Add(c => c.ts_breakdownStart).Titled("ts_Start");
    columns.Add(c => c.ts_breakdownEnd).Titled("ts_End");
    //
    columns.Add(c => c.rt_alarm.protRecord_ID).Titled("protRecord_ID");
    columns.Add(c => c.rt_alarm.C_timestamp).Titled("ts_rt_alarm");
    columns.Add(c => c.rt_alarm.errorCode1).Titled("errorCode1");
    columns.Add(c => c.rt_alarm.errorCode1_txt).Titled("errorCode1_txt");
    columns.Add(c => c.rt_alarm.errorCode2).Titled("errorCode2");
    columns.Add(c => c.rt_alarm.errorCode2_txt).Titled("errorCode2_txt");
    columns.Add(c => c.rt_alarm.isError).Titled("isError");
    columns.Add(c => c.rt_alarm.isError_txt).Titled("isError_txt");


}).Filterable().Sortable().Pageable(pager =>
{
    pager.RowsPerPage = 20;
    pager.PageSizes = new Dictionary<Int32, String> { { 0, "All" }, { 20, "20" }, { 40, "40" }, { 80, "80" }, { 100, "100" } };
    pager.ShowPageSizes = true;
}).Empty("Did not find anything").Css("table-hover")
