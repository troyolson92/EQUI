@model IQueryable<EqUiWebUi.Areas.UltraLog.Models.h_measurementRemarks>

@Html.Grid(Model).Named("h_measurementRemarksGrid").Build(columns =>
{
    columns.Add(c => c.id).Titled("id").Css("d-none");
    columns.Add(c => c.InspectionPlanname).Titled("Plan");
    columns.Add(c => c.spotname).Titled("spotname");
    columns.Add(c => c.measuredThickness).Titled("measuredThickness");
    columns.Add(c => c.EvaluationClass).Titled("EvaluationClass");
    columns.Add(c => c.Autocomment).Titled("Autocomment");
    columns.Add(c => c.InspectorComment).Titled("InspectorComment");
    columns.Add(c => c.ULDateTime).Titled("ULDateTime");

}).Sortable().Filterable().Empty("No data found").Css("table-hover")
