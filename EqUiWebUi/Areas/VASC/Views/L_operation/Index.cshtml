@model IEnumerable<EqUiWebUi.Areas.VASC.Models.L_operation>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@if (ViewBag.controller_id != null)
{
    <h3>Filtered on controllerID: @ViewBag.controller_id</h3>
}

<div class="container">
    <h2>Index L_operation</h2>
    <div class="alert alert-info">
        <h4>
            L_operation
        </h4>

        <div class="text-muted">
        </div>
    </div>
</div>

@Html.AjaxGrid(Url.Action("_List", new { controller_id = ViewBag.controller_id, sessionName = ViewBag.sessionName }))

<hr />

@section scripts{
    <script>
        //full with screen
        $("#allcontent").removeClass("body-content");
        $("#allcontent").removeClass("container");
    </script>
}

