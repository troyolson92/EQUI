@model List<EqUiWebUi.Areas.VASC.Models.winService>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@Html.Grid(Model).Named("c_controller_class").Build(columns =>
{
    columns.Add(c => c.ServiceName).Titled("ServiceName");
    columns.Add(c => c.DisplayName).Titled("DisplayName");
    columns.Add(c => c.StartName).Titled("StartName");
    columns.Add(c => c.Description).Titled("Description");
    columns.Add(c => c)
       .Titled("Status")
       .Encoded(false)
       .Filterable(true)
       .RenderedAs(c => MyFilters.SetServiceState(c.ServiceName, c.Status));

}).Filterable().Sortable().Pageable(pager =>
{
    pager.RowsPerPage = 10;
}).Empty("Did not find anything").Css("table-hover")
